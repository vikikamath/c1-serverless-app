{
  "name": "@sam-app/vue-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "unit": "jest",
    "e2e": "echo \"No e2e configured\"",
    "test": "yarn unit && yarn e2e",
    "validate": "sam validate -t ui.yaml",
    "package": "sam package --template-file ui.yaml --s3-bucket c1-serverless-app-deploy --output-template-file ui.packaged.yaml --debug",
    "sync": "VUE_APP_API_URL=`aws cloudformation describe-stacks --stack-name c1-serverless-api --query 'Stacks[0].Outputs[0].OutputValue'` yarn build && aws s3 sync dist s3://c1-serverless-app",
    "deploy": "sam deploy --template-file ui.packaged.yaml --stack-name c1-serverless-ui"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap-vue": "^2.0.0-rc.26",
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7"
  },
  "devDependencies": {
    "@sam-app/api": "*",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
